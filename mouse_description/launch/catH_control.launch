<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <rosparam file="$(find mouse_description)/config/mouse.yaml"
            command="load"/>

  <node name="robot_state_publisher_catH" pkg="robot_state_publisher" type="robot_state_publisher"
        respawn="false" output="screen">
            <param name="publish_frequency" type="double" value="30.0" />
            <param name="ignore_timestamp" type="bool" value="true" />
            <param name="tf_prefix" type="string" value="catH" />
            <remap from="/joint_states" to="/catH/joint_states" />
        </node>


  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
            args="--namespace=/catH
                      joint_state_controller
                      left_diff_drive_controller
                      right_diff_drive_controller">
  </node>

  <node name="catH_odom_pubish_node"
        pkg="mouse_description"
        type="get_gazebo_model_odometry.py" respawn="false"
        output="screen" args="catH">
  </node>

</launch>
